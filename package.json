{
  "name": "typey-boilerplate",
  "version": "0.0.1",
  "description": "a better typescript project",
  "main": "app/index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEBUG=true webpack-dev-server --mode development",
    "build:clean": "rm -rf dist && mkdir dist",
    "build": "npm run build:clean && cross-env NODE_ENV=production DEBUG=false webpack --mode production --progress --colors",
    "prettier": "prettier --write '*/*.{ts,tsx,js,jsx,css,less,json}'",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit && eslint 'app/*.{ts,tsx,js,jsx,css,less,json}' --quiet --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/history": "^4.7.5",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "babel-loader": "^8.0.6",
    "babel-plugin-const-enum": "0.0.5",
    "connected-react-router": "^6.7.0",
    "core-js": "^3.6.4",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "raf": "^3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "redux": "^4.0.5",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "autoprefixer": "^9.7.5",
    "circular-dependency-plugin": "^5.2.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
